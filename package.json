{
  "name": "my-buddy-gymmie",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.1.4",
    "apollo-client": "^2.2.8",
    "font-awesome": "^4.7.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "lodash": "^4.17.21",
    "material-ui": "^0.20.0",
    "moment": "^2.22.1",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-apollo": "^2.1.3",
    "react-burger-menu": "^2.4.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-spinners": "^0.3.2",
    "recompose": "^0.27.0",
    "redux": "^4.0.0",
    "redux-form": "^7.3.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "start": "PORT=8081 NODE_ENV=development react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --maxWorkers=4",
    "devtest": "react-scripts test --env=jsdom --watch --coverage",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint src/components src/actions src/api src/reducers src/utils",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy": "now --token $NOW_TOKEN rm gymmie -y && now build -e NODE_ENV=production --name gymmie --public --token $NOW_TOKEN && now --token $NOW_TOKEN alias"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.2",
    "@storybook/addon-links": "^3.4.2",
    "@storybook/addons": "^3.4.2",
    "@storybook/react": "^3.4.2",
    "babel-core": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "fetch-mock": "^6.3.0",
    "jest-each": "^0.3.1",
    "redux-mock-store": "^1.5.1"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "now": {
    "alias": "gymmie-abandisch",
    "name": "gymmie"
  }
}

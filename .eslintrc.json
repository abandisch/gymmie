{
    "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "jest/globals": true
    },
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "import/no-extraneous-dependencies": [2, { "devDependencies": true }],
      "react/no-unused-prop-types": 0
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": ["jest"],
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".js" ]
        }
      }
    }
  }